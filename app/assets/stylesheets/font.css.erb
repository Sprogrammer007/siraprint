@font-face {
  font-family: 'ETmodules';
  src: url("<%= font_path('ETmodules.eot') %>");
  src: url("<%= font_path('ETmodules.eot?#iefix') %>")  format("embedded-opentype"), 
       url("<%= font_path('ETmodules.ttf') %>") format("truetype"), 
       url("<%= font_path('ETmodules.woff') %>") format("woff"), 
       url("<%= font_path('ETmodules.svg#ETmodules') %>") format("svg");
  font-weight: normal;
  font-style: normal;
}